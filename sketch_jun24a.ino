int LED1 = 8;
int LED2 = 7;
int LED3 = 6;


void setup() {
  // put your setup code here, to run once:
pinMode(LED1,OUTPUT);
pinMode(LED2,OUTPUT);
pinMode(LED3,OUTPUT);

}

void loop() {
  // put your main code here, to run repeatedly:
digitalWrite(LED1,HIGH);
digitalWrite(LED2,HIGH);
digitalWrite(LED3,HIGH);

}
